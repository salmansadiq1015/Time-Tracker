    {
      "name": "Tasks",
      "item": [
        {
          "name": "List Tasks",
          "request": {
            "method": "GET",
            "header": [
              { "key": "Authorization", "value": "{{token}}", "type": "text" }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/v1/tasks?page=1&limit=10&status=all&priority=all&project=all&assignedTo=&search=",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "tasks"],
              "query": [
                {"key": "page", "value": "1"},
                {"key": "limit", "value": "10"},
                {"key": "status", "value": "all"},
                {"key": "priority", "value": "all"},
                {"key": "project", "value": "all"},
                {"key": "assignedTo", "value": ""},
                {"key": "search", "value": ""}
              ]
            },
            "description": "List all tasks. Users with 'user' role only see tasks assigned to them or created by them. Admins and dispatchers see all tasks. Supports filtering by status, priority, project, assignedTo, and search query."
          }
        },
        {
          "name": "Create Task",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Authorization", "value": "{{token}}", "type": "text" },
              { "key": "Content-Type", "value": "application/json" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"title\": \"Complete project documentation\",\n  \"description\": \"Write comprehensive documentation for the project\",\n  \"project\": \"{{projectId}}\",\n  \"assignedTo\": \"{{userId}}\",\n  \"status\": \"pending\",\n  \"priority\": \"high\",\n  \"dueDate\": \"2025-02-15T00:00:00.000Z\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/v1/tasks",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "tasks"]
            },
            "description": "Create a new task. Title, description, project, and dueDate are required. assignedTo is optional. Status defaults to 'pending', priority defaults to 'medium'."
          }
        },
        {
          "name": "Get Task Details",
          "request": {
            "method": "GET",
            "header": [
